\": 135997, \"offset\": \"05307231\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"(prosody) the accent in a metrical foot of verse\", \"semfield\": \"2657\"}, {\"id\": 135998, \"offset\": \"05307412\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"analysis of verse into metrical patterns\", \"semfield\": \"2657\"}, {\"id\": 135999, \"offset\": \"05307507\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"a poetic rhythm that imitates the rhythm of speech\", \"semfield\": \"2657\"}, {\"id\": 136000, \"offset\": \"05307617\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"the usual (iambic) meter of a ballad\", \"semfield\": \"2657\"}, {\"id\": 136001, \"offset\": \"05307729\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"a group of 2 or 3 syllables forming the basic unit of poetic rhythm\", \"semfield\": \"2657\"}, {\"id\": 136002, \"offset\": \"05308005\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"a metrical unit with stressed-unstressed-unstressed syllables\", \"semfield\": \"2657\"}, {\"id\": 136003, \"offset\": \"05308119\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"a metrical unit with unstressed-stressed syllables\", \"semfield\": \"2657\"}, {\"id\": 136004, \"offset\": \"05308229\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"a metrical unit with unstressed-unstressed-stressed syllables\", \"semfield\": \"2657\"}, {\"id\": 136005, \"offset\": \"05308355\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"a metrical unit with unstressed-stressed-unstressed syllables (e.g., \\\"remember\\\")\", \"semfield\": \"2657\"}, {\"id\": 136006, \"offset\": \"05308492\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"a metrical unit with stressed-stressed-unstressed syllables\", \"semfield\": \"2657\"}, {\"id\": 136007, \"offset\": \"05308605\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"a metrical unit with stressed-stressed syllables\", \"semfield\": \"2657\"}, {\"id\": 136008, \"offset\": \"05308707\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"a metrical unit with unstressed-unstressed syllables\", \"semfield\": \"2657\"}, {\"id\": 136009, \"offset\": \"05308823\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"a verse line having four metrical feet\", \"semfield\": \"1969\"}, {\"id\": 136010, \"offset\": \"05308918\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"a verse line having five metrical feet\", \"semfield\": \"1969\"}, {\"id\": 136011, \"offset\": \"05309013\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"a verse line having six metrical feet\", \"semfield\": \"1969\"}, {\"id\": 136012, \"offset\": \"05309106\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"a verse line having eight metrical feet\", \"semfield\": \"1969\"}, {\"id\": 136013, \"offset\": \"05309201\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"a verse line having eight syllables or a poem of octosyllabic lines\", \"semfield\": \"1969\"}, {\"id\": 136014, \"offset\": \"05309327\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"a verse line having ten syllables\", \"semfield\": \"1969\"}, {\"id\": 136015, \"offset\": \"05309419\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"correspondence in the sounds of two or more lines (esp. final sounds)\", \"semfield\": \"2657\"}, {\"id\": 136016, \"offset\": \"05309656\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"a rhyme between words in the same line\", \"semfield\": \"2657\"}, {\"id\": 136017, \"offset\": \"05309755\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"use of the same consonant at the beginning of each stressed syllable in a line of verse: \\\"around the rock the ragged rascal ran\\\"\", \"semfield\": \"2657\"}, {\"id\": 136018, \"offset\": \"05309989\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"the repetition of similar vowels in the stressed syllables of successive words\", \"semfield\": \"2657\"}, {\"id\": 136187, \"offset\": \"05335856\", \"pos\": \"n\", \"language\": 10, \"gloss\": \"the same pronunciation for words of different origins\", \"semfield\